@model HomeViewModel
@{
    ViewData["Title"] = "Home Page";
    var language = _localization.Getkey("Language").Value;
    var count = 0;
}

<!-- Contact Us Button -->
<div class="contactUsBtnBox " id="hereket">
    <a href="#" data-bs-toggle="modal" data-bs-target="#contactUsModal" id="ctnBtn">
        <i class="fa-solid fa-phone-volume "></i>
    </a>
</div>

<!-- Revo Slider -->
<div class="slider">
    @foreach (var item in Model.RevolutionSliders)
    {
        <div class="slide @(count == 0?"active":"")">
            <img src="~/uploads/revolutionSlider/@(item?.Image) " alt="Resim 1">
        </div>
        @(count++)
    }
</div>

<!-- XBInsaat Web Size -->
<div class="xbinsaat-web">
    <!-- Boxs -->
    <section id="sliderBoxs">
        <div class=" sliderRangeBox">
            <div class=" revolutionSliderclass " style="display: flex; justify-content: end; ">
                <div class="kutus d-flex">
                    <div class="kutu generalKutu">
                        <a href="#" data-bs-toggle="modal" data-bs-target="#generalModalScrollable">
                            <div class="kutuFeature" style="background-color: #000;" onmouseover="showSlide(0)">
                                <span style="color: white;">
                                    @Html.Raw(Model.Settings.FirstOrDefault(x => x.Key == "BoxGeneralTitle" + language)?.Value)
                                </span>
                            </div>
                        </a>
                    </div>
                    <div class="kutu servicesKutu">
                        <a href="#" data-bs-toggle="modal" data-bs-target="#servicesModalScrollable">
                            <div class="kutuFeature" style="background-color: #fff;" onmouseover="showSlide(1)">
                                <span style="color: black;">@Html.Raw(Model.Settings.FirstOrDefault(x => x.Key == "BoxServicesTitle" + language)?.Value)</span>
                            </div>
                        </a>
                    </div>
                    <div class="kutu projectsKutu">
                        <a href="#" data-bs-toggle="modal" data-bs-target="#projectsModalScrollable">
